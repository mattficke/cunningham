<div class="container">
  <div class="fixed">
    <button id="get">Get Location</button>

    <div id='map'></div>
  </div>
</div>

<div class="container">
  <div class="photo-map">
    <h1><%= @feed.name %></h1>
    <%= link_to edit_feed_path do %>
      <span class="glyphicon glyphicon-edit"></span>
    <% end %>
    <%= form_for @feed, method: :delete do |f| %>
      <input type="hidden", name="unsubscribe_id", value="<%= @feed.id %>">
      <%= f.submit "Unsubscribe" %>
    <% end %>
    <div class="row">
      <div class="col-md-12">
        <% @media.each do |media| %>
          <%= link_to image_path media.link, :class => "thumbnail" do%>
                  <%= image_tag media.images.low_resolution.url %>
                <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>
